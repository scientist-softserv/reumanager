.row
  .col-2
  .col-2
    %h2.float-left Applications
.row
  .col-2
  .col-8
    %table.table
      %thead
        %tr
          %th Started
          %th Completed
          %th Accepted
          %th Rejected
          %th Withdrawn
          %th Total
      %tbody
        %tr
          %td= @countStarted
          %td= @countCompleted
          %td= @countAccepted
          %td= @countRejected
          %td= @countWithdrawn
          %td= @sum
    .col-2

.row
  .col-2
  .col-2
    %h2.float-left Details
.row
  .col-2
  .col-8
    %table.table
      %thead
        %tr
          %th Application Start Date
          %th Application Deadline
          %th Notification Date
          %th Program Start Date
          %th Program End Date
          %th University
          %th Department
      %tbody
        - @values.each do |value|
          %td= value.value
  .col-2



